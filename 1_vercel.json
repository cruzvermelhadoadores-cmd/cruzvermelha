{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "vercel-node@3.0.0",
      "includeFiles": "dist/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
