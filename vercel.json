{
  "version": 2,
  "functions": {
    "server/index.js": {
      "runtime": "@vercel/node",
      "includeFiles": [
        "dist/**",
        "server/**",
        "shared/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
